programa {
  funcao inicio() {
    inteiro a, b, soma, sub, mult

    escreva("Digite um número: ")
    leia(a)

    escreva("Digite um segundo número: ")
    leia(b)

    soma = b + a
    escreva("O resultado da soma é: ", soma)

    sub = a - b
    escreva("O resultado da subtração é: ", sub)
    
    mult = a * b
    escreva ("O resultado da multiplicação é:", mult)
  }
}
